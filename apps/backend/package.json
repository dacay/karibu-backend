{
  "name": "karibu-backend",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:qmd": "concurrently \"pnpm dev\" \"chokidar 'src/**/*' -c 'qmd embed'\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed:dev": "tsx src/scripts/seed.dev.ts"
  },
  "dependencies": {
    "@ai-sdk/elevenlabs": "^2.0.20",
    "@ai-sdk/openai": "^3.0.29",
    "@aws-sdk/client-s3": "^3.995.0",
    "@aws-sdk/s3-request-presigner": "^3.995.0",
    "@hono/node-server": "^1.19.9",
    "@hono/zod-validator": "^0.7.6",
    "@mastra/core": "^1.4.0",
    "@sentry/node": "^10.38.0",
    "ai": "^6.0.89",
    "bcryptjs": "^3.0.3",
    "chromadb": "^3.3.1",
    "dotenv": "^17.2.4",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.8",
    "hono-pino": "^0.10.3",
    "ms": "^2.1.3",
    "pino": "^10.3.0",
    "postgres": "^3.4.8",
    "twilio": "^5.12.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.11.17",
    "@types/pg": "^8.16.0",
    "@types/twilio": "^3.19.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
